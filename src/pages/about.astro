---
import PostLayout from "@/layouts/BlogPost";
import { getConfigPage } from "../utils/config";
import Comment from "@/components/Comment";


const about = await getConfigPage('about')
const { Content } = await about.render();
---
<!--@ts-ignore-->
<PostLayout post={about}>
  <Content/>
  <div class="mt-32"></div>
  {comment && <Comment />}
</PostLayout>
